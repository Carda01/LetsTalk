@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://sdm_upc.org/ontology/Entertainment_News> rdfs:subClassOf <http://sdm_upc.org/ontology/News> .

<http://sdm_upc.org/ontology/Sports_News> rdfs:subClassOf <http://sdm_upc.org/ontology/News> .

<http://sdm_upc.org/ontology/Tech_News> rdfs:subClassOf <http://sdm_upc.org/ontology/News> .

<http://sdm_upc.org/ontology/author_name> rdfs:domain <http://sdm_upc.org/ontology/Author> ;
    rdfs:range xsd:string .

<http://sdm_upc.org/ontology/genre_name> rdfs:domain <http://sdm_upc.org/ontology/Genre> ;
    rdfs:range xsd:string .

<http://sdm_upc.org/ontology/has_genre> rdfs:domain <http://sdm_upc.org/ontology/Genre> ;
    rdfs:range <http://sdm_upc.org/ontology/Movie> .

<http://sdm_upc.org/ontology/keyword_text> rdfs:domain <http://sdm_upc.org/ontology/Keyword> ;
    rdfs:range xsd:string .

<http://sdm_upc.org/ontology/likes_movie> rdfs:domain <http://sdm_upc.org/ontology/User> ;
    rdfs:range <http://sdm_upc.org/ontology/Movie> .

<http://sdm_upc.org/ontology/movie_adult> rdfs:domain <http://sdm_upc.org/ontology/Movie> ;
    rdfs:range xsd:boolean .

<http://sdm_upc.org/ontology/movie_budget> rdfs:domain <http://sdm_upc.org/ontology/Movie> ;
    rdfs:range xsd:integer .

<http://sdm_upc.org/ontology/movie_language> rdfs:domain <http://sdm_upc.org/ontology/Movie> ;
    rdfs:range xsd:string .

<http://sdm_upc.org/ontology/movie_popularity> rdfs:domain <http://sdm_upc.org/ontology/Movie> ;
    rdfs:range xsd:long .

<http://sdm_upc.org/ontology/movie_release_date> rdfs:domain <http://sdm_upc.org/ontology/Movie> ;
    rdfs:range xsd:date .

<http://sdm_upc.org/ontology/movie_revenue> rdfs:domain <http://sdm_upc.org/ontology/Movie> ;
    rdfs:range xsd:integer .

<http://sdm_upc.org/ontology/movie_runtime> rdfs:domain <http://sdm_upc.org/ontology/Movie> ;
    rdfs:range xsd:integer .

<http://sdm_upc.org/ontology/movie_title> rdfs:domain <http://sdm_upc.org/ontology/Movie> ;
    rdfs:range xsd:string .

<http://sdm_upc.org/ontology/movie_vote_avg> rdfs:domain <http://sdm_upc.org/ontology/Movie> ;
    rdfs:range xsd:long .

<http://sdm_upc.org/ontology/movie_vote_cnt> rdfs:domain <http://sdm_upc.org/ontology/Movie> ;
    rdfs:range xsd:integer .

<http://sdm_upc.org/ontology/news_date> rdfs:domain <http://sdm_upc.org/ontology/News> ;
    rdfs:range xsd:dateTime .

<http://sdm_upc.org/ontology/news_title> rdfs:domain <http://sdm_upc.org/ontology/News> ;
    rdfs:range xsd:string .

<http://sdm_upc.org/ontology/published_at> rdfs:domain <http://sdm_upc.org/ontology/News> ;
    rdfs:range <http://sdm_upc.org/ontology/Source> .

<http://sdm_upc.org/ontology/related_keyword> rdfs:domain <http://sdm_upc.org/ontology/News> ;
    rdfs:range <http://sdm_upc.org/ontology/Keyword> .

<http://sdm_upc.org/ontology/source_name> rdfs:domain <http://sdm_upc.org/ontology/Source> ;
    rdfs:range xsd:string .

<http://sdm_upc.org/ontology/user_name> rdfs:domain <http://sdm_upc.org/ontology/User> ;
    rdfs:range xsd:string .

<http://sdm_upc.org/ontology/written_by> rdfs:domain <http://sdm_upc.org/ontology/News> ;
    rdfs:range <http://sdm_upc.org/ontology/Author> .

